
@{
    ViewData["Title"] = "SecondTimeError";
}
@section scripts {
    <link rel="stylesheet" href="~/Content/css/consent.css" />
    <script src="~/Scripts/SecondTimeError.js"></script>
}

<body>
    <div id="div_body">


        <div id="Consent_Form">
            <p>
                Sorry, you have already participated in this HIT in the past. Please return this HIT.
